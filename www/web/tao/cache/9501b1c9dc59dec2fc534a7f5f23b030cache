<script type="text/javascript">
    
    
    var $classAid = {
        page:1,
        firstEvent:false,
        ajaxUrl:false,
        isGetAjax : false,
        pageStatus : true,
        cac_id:"",
        getClassAidList : function(url,data,callback){
            var _this = this;
            if($classAid.isGetAjax) return false;
            var item = $cmsLayer.getLoadItem("nine"+_this.page+url);
            if(item){
                callback(item);
                return false;
            }
            $classAid.isGetAjax = true;
            var indexLoad = $cmsLayer.load();
            return $.ajax({
                url:url,
                type: 'GET',
                dataType: 'json',
                data:data ,
            }).error(function() {
                $cmsLayer.msg("服务器繁忙请稍后再试！");
                indexLoad.hide();
                $classAid.isGetAjax = false;
            }).done(function(data) {
                indexLoad.hide();
                $classAid.isGetAjax = false;                
                $cmsLayer.setLoadItem("nine"+_this.page+url,data);
                callback(data);
                return data;
            });
        },
        showClassAid : function(data){
            var _this = this;
            $classAid.ajaxUrl=data;
            if(!$classAid.pageStatus){
                $cmsLayer.msg("已最后一页了！");
                return false;
            }
            var getClassAidListPromise = $classAid.getClassAidList(data,{page:$classAid.page,'cac_id':this.cac_id},function(item){
                if($classAid.page==1){
                    $(".goods-new").html("");
                } 
                if($classAid.firstEvent==$classAid.ajaxUrl){
                    $(".nine_comlist").show();
                }else{
                    $(".nine_comlist").hide();
                }
                _this.cac_id = item.data.cac_id;
                // 这里调用 cms_common 公用商品列表模板渲染 
                var list = $($cmsLayer.goodsListTpl(item.data.data,"/tao?r=p/d","http://cmsstatic.dataoke.com/")).addClass('loadPage'+$classAid.page); 
                $classAid.pageStatus = item.data.pageStatus;     
                $(".goods-new").append(list);          
                $('.loadPage'+$classAid.page + " img").lazyload({
                    effect: "fadeIn"
                }); 
      
                $classAid.page = $classAid.page + 1;
                
            });
        }
    };
    $(window).resize(function(){
        setResize();
    })  
    var setResize = function(){
        var splist = $(".nine_swiper_new div.swiper-slide a.img img");
        splist.css({"height":splist.width()});
        $(".nine_tab li").css({"height":$(window).width()/4});
        //$(".icon_nav_tab").css({"top":$(".icon_nav_tab_bg").offset().top,"position":"absolute"});
        
        $.each($(".nine_swiper_new div.swiper-slide .new"),function(index, el) {
            var h = $(this).parents(".swiper-wrapper").find("div.swiper-slide:eq(1)").height();
            if(h!=0){
                $(this).removeAttr('style').css({"height":$(this).parents(".swiper-wrapper").find("div.swiper-slide:eq(1)").height()});
            }
        });
    };
    setResize();
    $(".nine_swiper_new").on("touchend",function(){
        setResize();
    });
    $(window).load(function(){
        var uiSwiper = new Swiper("div[ui-swiper]", {
            slidesPerView : 'auto',
            initialSlide :0
        });
         
        setResize();  
        MtaH5.pgv();
        MtaH5.clickStat("showpage",{
            uid:$cmsLayer.getMtaCookie(),
            time:((new Date()).getHours()<10 ? "0" + (new Date()).getHours() : (new Date()).getHours())+":"+((new Date()).getMinutes()<10 ? "0" + (new Date()).getMinutes() : (new Date()).getMinutes()),
            name:"9块9"
        })
    });
    
    $(document).scroll(function(){
        var orderNavBgTop = $("#goods_sticky").offset().top;
        if($(document).scrollTop()+44 >= orderNavBgTop){ 
            $(".icon_nav_tab").addClass('fixed');
            $(".icon_nav_tab_bg").show();
        }else{
            $(".icon_nav_tab").removeClass('fixed');
            $(".icon_nav_tab_bg").hide();
        }
        if ($(document).scrollTop() >= $(document).height() - $(window).height() - 150 && $classAid.ajaxUrl) {
            $classAid.showClassAid($classAid.ajaxUrl,$classAid.page,"http://cmsstatic.dataoke.com/");
        }
    });

    // 点击回调
    var classCallback = function(url,name){
        $classAid.page = 1;
        $classAid.pageStatus = true;
        $classAid.cac_id = "";
        $classAid.showClassAid(url);
        $(document).scrollTop($("#goods_sticky").offset().top-43);
        $("title").html(name+"_9块9包邮");
        MtaH5.pgv();
        MtaH5.clickStat("showpage",{
            uid:$cmsLayer.getMtaCookie(),
            time:((new Date()).getHours()<10 ? "0" + (new Date()).getHours() : (new Date()).getHours())+":"+((new Date()).getMinutes()<10 ? "0" + (new Date()).getMinutes() : (new Date()).getMinutes()),
            name:"9块9"
        })
    }
    // 首次回调
    var classReady = function(url){
       $classAid.page = 1;       
       $classAid.firstEvent = url;
       $classAid.showClassAid(url); 
    };

    $("img.lazy").lazyload({
        effect: "fadeIn"
    }); 
</script><!-- 主界面具体展示内容e -->


