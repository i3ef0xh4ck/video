<div id="div1"></div><script>window.onload=function(){
        function timer(){
            var now=new Date();
            var endTime=new Date("2018/8/18,0:0:0");
            var ed=Math.floor((endTime-now)/1000);
            var d=Math.floor(ed/60/60/24);
            var h=Math.floor(ed/60/60%24);
            var m=Math.floor(ed/60%60);
            var s=Math.floor(ed%60);   
           
            if(h<10){h='0'+h};
            if (m<10) {m='0'+m};
            if (s<10) {s='0'+s};

            var obj=document.getElementById("div1");
          obj.innerHTML=" 距离 8.18 还有： "+d+"天"+h+"小时"+m+"分"+s+"秒 ";
        }
  timer();
        setInterval(timer,1000);
        
    };
