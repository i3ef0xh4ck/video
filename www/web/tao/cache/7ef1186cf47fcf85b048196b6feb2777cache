for(var i = 0 ; i < $('.zt-ol-mod').length ; i ++){
    var parDom = $('.zt-ol-mod').eq(i);
    if(goodsJson[parDom.data('modid')]){
        var  goodsArr = goodsJson[parDom.data('modid')];
        for(var j = 0 ; j < goodsArr.goods.length ; j ++ ){
            if(j < goodsArr.goods_num){
                var domStr = goodsArr.template,goods = goodsArr.goods[j];
                domStr =  domStr.replace('#gid#',goods.id);//商品大淘id
                domStr =  domStr.replace('#detail_link#',goodsArr.title_link?goodsArr.title_link+goods.id:'javascript:void()');//商品大淘链接

                if(j > 4){
                    domStr =  domStr.replace('#lazy_pic#','http://cmsstatic.dataoke.com//web/images/rolling.gif?v=201711071440');//懒加载图片
                }else{
                    domStr =  domStr.replace('#lazy_pic#',goods.pic+'_310x310.jpg');//商品图片
                }

                domStr =  domStr.replace('#pic#',goods.pic+'_310x310.jpg');//商品图片


                domStr =  domStr.replace('#title#',goods.d_title);//商品标题
                domStr =  domStr.replace('#price#',parseFloat((goods.yuanjia - goods.quan_jine).toFixed(2)));//券后价
                domStr =  domStr.replace('#quan_getprice#',parseFloat(goods.jiage));//到手价
                domStr =  domStr.replace('#old_price#', parseFloat(goods.yuanjia));//原价
                domStr =  domStr.replace('#xiaoliang#',goods.xiaoliang);//销量
                domStr =  domStr.replace("#detail_miaoshu#",goods.miaoshu);//商品描述
                domStr =  domStr.replace('#ding_xiaoliang#',(goods.xiaoliang<10000?goods.xiaoliang:parseFloat(goods.xiaoliang/10000).toFixed(1)+'万'));//已预定
                domStr =  domStr.replace('#quan_price#',parseFloat(goods.quan_jine));//券金额
                domStr =  domStr.replace('#top_num#',(j+1));//排序值
                var expend = parseFloat(goods.expend);
                var earning= parseFloat(goods.earning);
                domStr =  domStr.replace('#expend#',isNaN(expend)?0:expend );//定金
                domStr =  domStr.replace('#earning#',isNaN(earning)?0:earning);//抵金额
        if (goods.istmall == 1){
            var original_price = '天猫价：'+goods.yuanjia;
        }else{
            var original_price = '淘宝价：'+goods.yuanjia;
        }
        domStr  =  domStr.replace('#original_price#',original_price );//天猫淘宝价

                parDom.find('.zt-ol-mod-list').append(domStr);
            }
        }

        var htm = parDom.html();
        htm = replaceAll(htm,goodsArr.title_link);

        if(var_tid == "app"){
            htm = htm.replace(/@@(\w+)@@/g, '/atopic/'+"$1");
            htm = htm.replace(/%%editor%%/g, '/index.php?r=recommend/index');
            htm = htm.replace(/%%home%%/g, '');
        }else{
            if(typeof(entrance)=="undefined"){
                var entrance = '';
            }
            htm = htm.replace(/@@(\w+)@@/g,entrance+'?r=a/t&i='+"$1");
            htm = htm.replace(/%%editor%%/g, entrance+'?r=index/e');
            htm = htm.replace(/%%home%%/g, entrance+'/');
        }
        parDom.html(htm);

        if(i == $('.zt-ol-mod').length-1){

            $("img.lazy").lazyload();
            $('.zt-ol-mod').each(function(){
                if($(this).find('.zt-ol-mod-list').find('.zt-ol-mod-item').length > goodsJson[$(this).data('modid')].app_threshold && goodsJson[$(this).data('modid')].app_threshold != null && goodsJson[$(this).data('modid')].app_threshold != 0){
                    var mod_main = $(this).find('.zt-ol-mod-main');
                    if (!mod_main.hasClass('slide')){
                        mod_main.append($('<div class="wap_goods_control goods_hide"> <span                           style="background:'+$(this).css('background')+'">展开更多</span></div>'));
                    }
                }
            });
            //横向滑动

        $('.zt-ol-mod-main.slide').each(function(){
            var _this = $(this);
            if(_this.find('.zt-ol-mod-item').length > 2){
                _this.parent().css({'height':_this.find('.zt-ol-mod-list').height()-8,'overflow-y':'hidden'});
                _this.css('height',_this.height());
                _this.find('.zt-ol-mod-item').css('display','block');

                var paddingLeft = parseFloat(_this.find('.zt-ol-mod-item').eq(0).css('paddingLeft'));
                var paddingRight = parseFloat(_this.find('.zt-ol-mod-item').eq(0).css('paddingRight'));
                var marginLeft = parseFloat(_this.find('.zt-ol-mod-item').eq(0).css('marginLeft'));
                var item_width = _this.find('.zt-ol-mod-item').eq(0).width();

                var this_width = marginLeft+item_width+paddingLeft+paddingRight+1;

                var item_num = _this.find('.zt-ol-mod-item').length;
                var num_amount = this_width*item_num;

                _this.find('.zt-ol-mod-list').css('width',num_amount);
                _this.find('.zt-ol-mod-item').css({'width':item_width,'margin-left':marginLeft,'margin-right':'0'});
            }
        });
            $('.wap_goods_control').on('click',function(){
                if($(this).hasClass('goods_hide')){
                    $(this).find('span').html('收起商品');
                    $(this).removeClass('goods_hide');
                    $(this).addClass('goods_show');
                    $(this).parents('.zt-ol-mod').find('.zt-ol-mod-list').addClass('wap_goods_more');
                    $("img.lazy").lazyload();

                }else{
                    $(this).find('span').html('展开更多');
                    $(this).removeClass('goods_show');
                    $(this).addClass('goods_hide');
                    $("html,body").animate({"scrollTop":($(this).parents('.zt-ol-mod').offset().top-50)},300);
                    $(this).parents('.zt-ol-mod').find('.zt-ol-mod-list').removeClass('wap_goods_more');
                }
            });
        }
