function replaceAll(str,prefix)
{
    if(str!=null)
        str = str.replace(/##(\d+)##/g,prefix+"$1")
    return str;
}
if($('.fast_menu .fast_menu_main li').length < 5){
    $('.fast_menu .fast_right').remove();
