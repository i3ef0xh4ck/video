                $(".toTop").fadeOut(1000);
            }
        });
    });

    $(window).scroll(function(){

        var _scroll = $(window).scrollTop();

        if(_scroll>130){
            $(".floatNav").fadeIn();
            $('.floatNav .fn-block').width(1200-$(".fn-left").width()-47);
            $('.floatNav .fn-block .my-src-input').width(1200-$(".fn-left").width()-147);
