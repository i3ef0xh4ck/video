        if($('.wap-src-cat').length>0){
            $(".close-src").on('click',function(){
                $(".search-pop").hide();
                $('.search-land').hide();
                $(".search-pop").find(".search_area").val('');
                $('body').unbind('touchmove')
                $('body').css('overflow-y','scroll');
                
            });
